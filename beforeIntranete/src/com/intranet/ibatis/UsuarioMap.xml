<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="usuario">

	<select id="buscarUsuario" parameterType="com.intranet.bean.Usuario"
		resultType="com.intranet.bean.Usuario">
		select *
		from usuario u
		where u.user = #{user} and
		u.password=#{password}
	</select>
</mapper>
